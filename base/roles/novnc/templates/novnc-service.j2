[Unit]
Description=HTML5 VNC Proxy (NOVNC)
After=syslog.target network.target

[Service]
Type=simple
User={{ novnc_user }}
PAMName=login
PIDFile=/home/%u/.vnc/%H%i.pid
ExecStart=/usr/local/share/noVNC/utils/launch.sh --vnc localhost:5901 --listen 8001
/opt/novnc/utils/launch.sh --vnc localhost:5901 --listen 5911 --cert $HOME/.vnc/certs/fullcert.pem --ssl-only

[Install]
WantedBy=multi-user.target
